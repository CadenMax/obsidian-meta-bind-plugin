{
	"compilerOptions": {
		"baseUrl": ".",
		"module": "ESNext",
		"target": "ES6",
		"allowJs": true,
		"noImplicitAny": true,
		"strict": true,
		"strictNullChecks": true,
		"noImplicitReturns": true,
		"moduleResolution": "node",
		"importHelpers": true,
		"isolatedModules": true,
		"lib": [
			"DOM",
			"ES5",
			"ES6",
			"ES7"
		],
		"types": ["svelte", "node"]
	},
	"include": [
		"**/*.ts"
	]
}
